{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
          "dependsOn": ["^build"],
          "outputs": ["dist/**", ".next/**"]
        },
        "web#build": {
          "dependsOn": [
            "^build",
            "$VERCEL_URL",
            "$RENDER_INTERNAL_HOSTNAME",
            "$PORT",
            "$GOOGLE_ID",
            "$GOOGLE_SECRET",
            "$NEXT_AUTH_SECRET"
          ],
          "outputs": [".next/**"]
        },
        "database:migrate:deploy": {
          "outputs": []
        },
        "database:push": {
          "outputs": []
        },
        "database:seed": {
          "outputs": []
        },
        "dev": {
          "cache": false
        },
        "generate": {
          "dependsOn": ["^generate"]
        },
        "lint": {
          "outputs": []
        }
      }
}
